{
  "manifest_version": 2,
  "name": "SEMI CICD TESTING",
  "version": "0.6.4.1004",
  "description": "QOL Scripting for SEMI REPO!",
  "icons": {
    "48": "icons/border-48.png",
    "128": "icons/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://melvoridle.com/*",
        "*://www.melvoridle.com/*",
        "*://test.melvoridle.com/*"
      ],
      "js": [
        "scripts/SEMI.js"
      ],
      "css": [
        "styles/semi.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "scripts/*.js",
    "icons/*.png",
    "scripts/plugins/*.js"
  ]
}
